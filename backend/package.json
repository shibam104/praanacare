{
  "name": "praanacare-backend",
  "version": "1.0.0",
  "description": "Backend API for PraanaCare MVP",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --exec \"ts-node\" src/server.ts",
    "build": "tsc --skipLibCheck",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "start:prod": "npm run build && npm start",
    "test": "jest",
    "instructions": "echo \"1) npm install && 2) create .env with MONGO_URI, JWT_SECRET, PORT, OPENAI_API_KEY, EMAIL_* && 3) npm run dev (dev) or npm run build && npm start (prod)\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "socket.io": "^4.7.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "healthcare",
    "ai",
    "industrial",
    "monitoring"
  ],
  "author": "PraanaCare Team",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
